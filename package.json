{
  "name": "leaflet-pather",
  "version": "0.1.17",
  "description": "Branching from Leaflet.FreeDraw, L.Pather is a freehand polyline creator that simplifies the polyline for mutability.",
  "main": "dist/leaflet-pather.cjs.js",
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup -c",
